<Page x:Class="CourseWork.Pages.ServicesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CourseWork.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="1800"  d:Width="1920"
      Title="ServicesPage">

    <StackPanel>
        <Grid Background="#0c1222">
            <Button x:Name="homeNavigationButton" Width="32" Height="32"
                    HorizontalAlignment="Right" VerticalAlignment="Top" 
                    Style="{StaticResource AzureSubButtonStyle}"
                    Click="HomeNavigationButton_Click"  Margin="5">
                <Image Source="/Icons/homeIcon.png"/>
            </Button>
        </Grid>

    </StackPanel>
</Page>
